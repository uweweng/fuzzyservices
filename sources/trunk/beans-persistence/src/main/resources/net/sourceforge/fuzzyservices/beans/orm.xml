    <package>net.sourceforge.fuzzyservices.beans</package>
    <entity class="net.sourceforge.fuzzyservices.beans.Operator" name="Operator">
        <table name="OPERATOR"/>
        <attributes>
            <id name="id">
                <generated-value strategy="AUTO"/>
            </id>
            <basic name="type"/>
            <basic name="parameter"/>
