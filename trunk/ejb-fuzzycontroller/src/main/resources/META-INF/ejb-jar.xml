<?xml version="1.0"?>
<!DOCTYPE ejb-jar PUBLIC '-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN' 'http://java.sun.com/dtd/ejb-jar_2_0.dtd'>
<ejb-jar>
    <display-name>JFuzzy Controller EJB</display-name>
    <enterprise-beans>
        <session>
            <display-name>Fuzzy Controller</display-name>
            <ejb-name>JFuzzyControllerEJB</ejb-name>
            <home>com.jfuzzy.ejbs.FuzzyControllerHome</home>
            <remote>com.jfuzzy.ejbs.FuzzyController</remote>
            <ejb-class>com.jfuzzy.ejbs.FuzzyControllerBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
    </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>JFuzzyControllerEJB</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>NotSupported</trans-attribute>
        </container-transaction>
    </assembly-descriptor>
</ejb-jar>
