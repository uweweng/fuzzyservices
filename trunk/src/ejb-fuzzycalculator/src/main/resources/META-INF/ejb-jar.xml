<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
    <display-name>Fuzzy Services Calculator EJB</display-name>
    <enterprise-beans>
        <session>
            <display-name/>
            <ejb-name>FuzzyServicesCalculatorEJB</ejb-name>
            <home>net.sourceforge.fuzzyservices.ejb.FuzzyCalculatorHome</home>
            <remote>net.sourceforge.fuzzyservices.ejb.FuzzyCalculator</remote>
            <ejb-class>net.sourceforge.fuzzyservices.ejb.FuzzyCalculatorBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <display-name>NewSessionSB</display-name>
            <ejb-name>NewSessionBean</ejb-name>
            <home>net.sourceforge.fuzzyservices.ejb.NewSessionRemoteHome</home>
            <remote>net.sourceforge.fuzzyservices.ejb.NewSessionRemote</remote>
            <ejb-class>net.sourceforge.fuzzyservices.ejb.NewSessionBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <display-name>NewSessionSB</display-name>
            <ejb-name>NewSessionBean</ejb-name>
            <home>net.sourceforge.fuzzyservices.ejb.NewSessionRemoteHome</home>
            <remote>net.sourceforge.fuzzyservices.ejb.NewSessionRemote</remote>
            <ejb-class>net.sourceforge.fuzzyservices.ejb.NewSessionBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <display-name>NewSessionSB</display-name>
            <ejb-name>NewSessionBean</ejb-name>
            <home>net.sourceforge.fuzzyservices.ejb.NewSessionRemoteHome</home>
            <remote>net.sourceforge.fuzzyservices.ejb.NewSessionRemote</remote>
            <ejb-class>net.sourceforge.fuzzyservices.ejb.NewSessionBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>FuzzyServicesCalculatorEJB</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>NotSupported</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>NewSessionBean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>NewSessionBean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>NewSessionBean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        </assembly-descriptor>
</ejb-jar>
